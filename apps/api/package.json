    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^22.5.4",
    "prisma": "^5.20.0",
    "tsx": "^4.19.0",
    "typescript": "^5.6.2"
  }
}
